generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model application_form {
  id                     Int       @id @default(autoincrement())
  application_id         BigInt?
  student_id             Int?
  university_id          Int?
  course_id              Int?
  personal_information   String?   @db.LongText
  educational_background String?   @db.LongText
  language_qualification String?   @db.LongText
  reasons_to_choose      String?   @db.LongText
  family                 String?   @db.LongText
  financial_support      String?   @db.LongText
  mailling_address       String?   @db.LongText
  declaration            Boolean?  @default(false)
  uploads                String?   @db.Text
  application_fee        Boolean?  @default(false)
  send_to_uni            Boolean   @default(false)
  status                 Int?      @default(4) @db.TinyInt
  display                Boolean   @default(true)
  created_at             DateTime? @db.Timestamp(0)
  updated_at             DateTime? @updatedAt @db.Timestamp(0)
}

model apply_now {
  id                Int      @id @default(autoincrement())
  name              String   @db.VarChar(255)
  city              String   @db.VarChar(255)
  phone_number      String   @db.VarChar(255)
  last_education    String   @db.VarChar(500)
  intrested_country String   @db.VarChar(255)
  created_at        DateTime @default(now()) @db.Timestamp(0)
  updated_at        DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @updatedAt @db.Timestamp(0)
}

model blogs {
  id                Int            @id @default(autoincrement())
  user_id           Int?
  category_id       Int?
  title             String?        @db.VarChar(255)
  sm_question       String?        @db.Text
  sm_answer         String?        @db.Text
  review_detail     String?        @db.Text
  rating_count      Int            @default(0)
  review_count      Int            @default(0)
  avg_review_value  Decimal        @default(0.00) @db.Decimal(12, 2)
  slug              String         @unique(map: "uq_blogs_slug") @db.VarChar(255)
  short_description String?        @db.Text
  description       String?        @db.LongText
  image             String?        @db.VarChar(255)
  image_ext         String?        @db.VarChar(50)
  is_active         Boolean        @default(true)
  seo               String?        @db.LongText
  views             Int            @default(0)
  likes             Int            @default(0)
  ip                String?        @db.LongText
  custom_post_type  String?        @db.VarChar(255)
  post_attributes   String?        @db.LongText
  is_featured       Boolean        @default(false)
  sort_order        Int?
  created_at        DateTime       @default(now()) @db.Timestamp(0)
  updated_at        DateTime       @default(now()) @updatedAt @db.Timestamp(0)
  enable_meta_tags  Boolean        @default(false)
  category          blog_category? @relation(fields: [category_id], references: [id], onDelete: Restrict, onUpdate: Restrict, map: "fk_blogs_category")
  user              users?         @relation(fields: [user_id], references: [id], onDelete: Restrict, onUpdate: Restrict, map: "fk_blogs_user")

  @@index([category_id], map: "blogs_category_id_fkey")
  @@index([user_id], map: "blogs_user_id_fkey")
}

model university_details {
  id                    Int        @id @default(autoincrement())
  sm_question           String     @db.Text
  sm_answer             String     @db.Text
  review_detail         String     @db.Text
  rating_count          Int        @default(0)
  review_count          Int        @default(0)
  avg_review_value      Decimal    @default(0.00) @db.Decimal(12, 2)
  user_id               Int?
  name                  String     @db.VarChar(255)
  slug                  String     @db.VarChar(255)
  founded_in            String?    @db.VarChar(255)
  area                  String?    @db.VarChar(255)
  country               String?    @db.VarChar(255)
  city                  String?    @db.VarChar(255)
  address               String?    @db.LongText
  postcode              String?    @db.VarChar(255)
  phone_no              BigInt?
  agency_number         String?    @db.VarChar(20)
  total_students        Int?
  international_student Int?
  scholarship           Boolean?   @default(false)
  logo                  String?    @db.VarChar(255)
  feature_image         String?    @db.Text
  about                 String?    @db.LongText
  guide                 String?    @db.LongText
  expanse               String?    @db.LongText
  languages             String?    @db.Text
  accommodation         String?    @db.VarChar(255)
  accommodation_detail  String?    @db.LongText
  intake                String?    @db.Text
  ranking               Float?     @default(0.00)
  designation           String?    @db.VarChar(255)
  alternate_email       String?    @db.VarChar(255)
  website               String?    @db.VarChar(255)
  active                Boolean    @default(true)
  popular               Boolean    @default(false)
  package               String     @default("free") @db.VarChar(255)
  expiry                DateTime?  @db.DateTime(0)
  display               Boolean    @default(true)
  created_at            DateTime   @default(now()) @db.Timestamp(0)
  updated_at            DateTime   @default(now()) @updatedAt @db.Timestamp(0)
  whishlist             wishlist[]
}

model blog_category {
  id          Int       @id @default(autoincrement())
  name        String?
  slug        String?
  description String?   @db.Text
  sort_order  Int?      @db.TinyInt
  is_active   Boolean?
  seo         String?   @db.LongText
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @updatedAt @db.Timestamp(0)
  blogs       blogs[]
}

model wishlist {
  id            Int                 @id @default(autoincrement())
  user_id       Int?
  course_id     Int?
  created_at    DateTime?           @db.Timestamp(0)
  updated_at    DateTime?           @db.Timestamp(0)
  university_id Int?
  course        courses?            @relation(fields: [course_id], references: [id])
  university    university_details? @relation(fields: [university_id], references: [id])
  user          users?              @relation(fields: [user_id], references: [id])

  @@index([user_id])
  @@index([course_id])
  @@index([university_id])
}

model students {
  id               Int       @id @default(autoincrement())
  user_id          Int?
