{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/(main)/free-consultation/page.tsx", "src/app/api/internal/university/route.ts", "src/app/api/internal/visa-types/route.ts", "src/app/api/internal/blogs/route.js", "src/app/(main)/guides/[slug]/page.jsx", "src/app/(main)/student-register/page.jsx", "src/app/components/clientcomponents/ArticleDetailPage.jsx", "src/app/components/organisms/Footer.jsx", "src/app/(main)/contact-us/page.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
